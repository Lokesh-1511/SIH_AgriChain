# Environment Configuration for Admin Dashboard

# Development Environment
VITE_APP_NAME=AgriChain Admin Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# API Endpoints (Replace with your actual endpoints)
VITE_BLOCKCHAIN_RPC_URL=http://localhost:8545
VITE_BACKEND_API_URL=http://localhost:3001/api

# Smart Contract Addresses (Replace with your actual contract addresses)
VITE_SUPPLY_CHAIN_CONTRACT=0x1234567890123456789012345678901234567890
VITE_TOKEN_CONTRACT=0x0987654321098765432109876543210987654321

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Demo Mode (Set to false for production)
VITE_DEMO_MODE=true

# Security (Don't include sensitive data in production)
VITE_DEMO_USERNAME=admin
VITE_DEMO_PASSWORD=admin123